def solution(rsp):
    answer = ''
    if (len(rsp) >= 2):
        for i in rsp:
            if (i == '2'):
                answer += '0'
            elif (i == '0'):
                answer += '5'
            else:
                answer += '2'
    else:
        if (rsp == '2'):
            answer += '0'
        elif (rsp == '0'):
            answer += '5'
        else:
            answer += '2'

    return answer
